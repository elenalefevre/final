<%= form_for @venue do |form| %>
  <div class="form-group">
    <%= form.label :name %>
    <%= form.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :street_number %>
    <%= form.text_field :street_number, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :street %>
    <%= form.text_field :street, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :neighborhood %>
    <%= form.text_field :neighborhood, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :zip %>
    <%= form.text_field :zip, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :city_id %>
    <%= form.collection_select :city_id, City.order(:name), :id, :name, { include_blank: true }, { class: "form-control" } %>
  </div>
  <div class="form-group">
    <%= form.label :state %>
    <%= form.text_field :state, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :latitude %>
    <%= form.text_field :latitude, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :longitude %>
    <%= form.text_field :longitude, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :dress_code %>
    <%= form.text_field :dress_code, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :hours %>
    <%= form.text_field :hours, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :website_url %>
    <%= form.text_field :website_url, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :google_maps_url %>
    <%= form.text_field :google_maps_url, class: "form-control" %>
  </div>
<%= form.button 'Update Venue', class: "btn btn-success" %>
<% end %>