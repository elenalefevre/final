<div class="row">
  <% @venues.each do |venue| %>
    <div class="col-sm-12 col-md-4">
      <div class="thumbnail">
        <%= link_to image_tag(venue.image), venue %>
        <div class="caption">
          <h3><%= link_to venue.name, venue %></h3>
        </div>
      </div>
    </div>
  <% end %>  
</div>